{
  "name": "pethealth",
  "private": true,
  "main": "index.js",
  "version": "0.0.1",
  "scripts": {
    "rn-cli": "node node_modules/react-native/local-cli/cli.js",
    "start": "npm run start:web",
    "start:android": "yarn rn-cli run-android",
    "start:windows": "yarn rn-cli run-windows",
    "start:ios": "yarn rn-cli run-ios",
    "start:web": "cross-env platform=web webpack-dev-server --config=web/webpack/dev.js --progress --colors --mode=development",
    "start:rn-dev-server": "yarn rn-cli start --reset-cache",
    "build:web": "cross-env platform=web webpack --config=web/webpack/prod.js --progress --colors --mode=production",
    "test": "jest -c jest/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest -c jest/jest.config.js --runInBand",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "tslint -p tsconfig.json -r tslint.json",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -w",
    "gen:graphql": "gql-gen"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/preset-env": "7.3.1",
    "@types/babel__core": "^7.0.5",
    "@types/compression-webpack-plugin": "2.0.0",
    "@types/enzyme": "3.1.16",
    "@types/enzyme-adapter-react-16": "1.0.4",
    "@types/enzyme-to-json": "1.5.2",
    "@types/graphql": "^14.0.7",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jest": "24.0.0",
    "@types/react": "16.7.21",
    "@types/react-dom": "16.8.1",
    "@types/react-native": "0.57.36",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-native": "^4.2.4",
    "@types/styled-components": "4.1.9",
    "@types/webpack": "4.4.24",
    "@types/webpack-dev-server": "3.1.2",
    "@types/webpack-merge": "4.1.3",
    "babel-loader": "8.0.5",
    "compression-webpack-plugin": "2.0.0",
    "cross-env": "5.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "enzyme-to-json": "3.3.5",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "graphql-code-generator": "^0.16.1",
    "graphql-codegen-typescript-client": "^0.16.1",
    "graphql-codegen-typescript-common": "^0.16.1",
    "graphql-codegen-typescript-react-apollo": "^0.16.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "rnpm-plugin-windows": "0.2.8",
    "tslint": "5.12.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.1",
    "webpack": "4.29.2",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "apollo-boost": "^0.1.28",
    "apollo-link-context": "^1.0.14",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "react": "16.7.0",
    "react-apollo": "^2.4.1",
    "react-dom": "16.7.0",
    "react-native": "0.57.8",
    "react-native-svg": "^9.2.4",
    "react-native-windows": "0.57.0-rc.5",
    "react-router": "^4.3.1",
